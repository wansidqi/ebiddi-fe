import { useMutation, useQuery, useQueryClient } from "@tanstack/react-query";

const useGetCredit = () => {
  const { USER } = useStoreContext();
  return useQuery({
    enabled: !!{{enable}},
    queryKey: [{{queryKey}}],
    queryFn: async () => {
      const response = await datasource({
        method: {{ method }},
        url: `{{url}}`,
      });
      const data = response.data;
      return data;
    },
    refetchOnWindowFocus: {{ refetchOnWindowFocus }},
  });
};

